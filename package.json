{
  "name": "react-native-svg-loader",
  "version": "0.1.3",
  "description": "Build time parsing for svgs, use any svg in your React Native app in ease",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint --ext .js,.jsx .",
    "lint:error": "npm run lint -- --quiet",
    "lint:fix": "npm run lint -- --fix",
    "lint:report": "npm run lint -- --format html -o ./report.html",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "build": "rimraf ./lib && node ./bin/build.js",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unimonkiez/react-native-svg-loader.git"
  },
  "keywords": [
    "react",
    "native",
    "svg",
    "webpack",
    "loader"
  ],
  "author": "Yuval Saraf",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/unimonkiez/react-native-svg-loader/issues"
  },
  "homepage": "https://github.com/unimonkiez/react-native-svg-loader#readme",
  "dependencies": {
    "xmldom": "^0.1.27"
  },
  "peerDependencies": {
    "prop-types": "*",
    "react": "*",
    "react-native": "*",
    "react-native-svg": "*"
  },
  "devDependencies": {
    "babel-eslint": "^8.1.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.12",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "jest": "^22.0.4",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-native": "^0.51.0",
    "react-native-svg": "^6.0.1-rc.1",
    "rimraf": "^2.6.2",
    "webpack": "^3.10.0",
    "webpack-node-externals": "^1.6.0"
  }
}
